.index{
    display: flex;
    height: 100%;
    width: 100%;
    flex-direction: column;
}

.title{
    display: flex;
    justify-content: space-around;
    align-items: center;
    background: white;

    .userinfo{
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
    }
}



.touxiang{
    margin: 10px 0px ;
}

.username{
    color: black;
}

.currenttime{
    font-size: 25px;
    color: black;
    font-weight: 500;
}

.center{
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    flex: 1;
}

.atheight{
    width: 100%;
}

.table{
    display: flex;
    width: 220*6px;
    align-items: center;
    flex-direction: column;
   
}

.tabletitle{
    display: flex;
    align-items: center;
    flex-direction: row;
    width: 220*6px;
}

.timelist{
    display: flex;
    align-items: center;
    flex-direction: column;
}

.itemtitle{
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    width: 220px;
    height: 50px;
    background: white;
    border-left: 1px solid  black;
    border-bottom: 1px solid  black;
    border-top: 1px solid  black;
    
}

.itemcontext{
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    width: 220px;
    background: white;
    border-left: 1px solid  black;
    border-top: 1px solid  black;
    height: 50px;
    overflow:hidden;
    text-overflow:ellipsis;
}

.itemcontext1{
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    width: 220px;
    background: white;
    height: 50px;
    overflow:hidden;
    text-overflow:ellipsis;
}

.noleftline{
    border-left: none;
}


.itemtitleplus{
    display: flex;
    flex-direction: row;
    width: 100%;
    height: 50px;
    margin-left:-1px;
}


.scrollview{
    border-left: 1px solid  black;
}
.scrollview1{
    border-left: 1px solid  black;
    width:220px;
    height:50px;
